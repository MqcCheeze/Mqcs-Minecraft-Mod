{
	"schemaVersion": 1,
	"id": "mqcs-minecraft-mod",
	"version": "${version}",
	"name": "Mqc's Minecraft Mod",
	"description": "A mod to learn Java",
	"authors": [
		"MqcCheeze"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mqcs-minecraft-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "net.mqc.minecraftmod.CustomBlocks"
		],
		"client": [
			"net.mqc.minecraftmod.CustomBlocksClient"
		],
		"fabric-datagen": [
          "net.mqc.minecraftmod.CustomBlocksDataGenerator"
		]
	},
	"mixins": [
		"mqcs-minecraft-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.18",
		"minecraft": "~1.19.3",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
